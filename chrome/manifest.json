{
  "manifest_version": 2,

  "name": "wordy",
  "description": "word frequency analysis",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["myscript.js","contentScript.js"]
    }
  ]
}
