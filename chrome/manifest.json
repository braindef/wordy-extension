{
  "manifest_version": 2,

  "name": "schwurbler extension",
  "description": "haeufigkeitsanaluese fuer Schwurbler Webseiten",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["myscript.js","contentScript.js"]
    }
  ]
}
